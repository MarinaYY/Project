    int slider1;
    int slider2;
    int slider3;
    int slider4;
    int slider5;
    private void jSlider1StateChanged(javax.swing.event.ChangeEvent evt) {                                      
        slider1 = jSlider1.getValue();
    }                                     
    private String result;
    int br;
    int i;
    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        int[] arr = new int[]{slider1, slider2, slider3, slider4, slider5};
        int max = 0;
        br = 0;

        for (int i = 0; i < arr.length; i++) {
            if (arr[i] > max) {
                max = arr[i];
                br=i;
            }
            
        }
        switch (br) {
            case 0:
                result = "Адвокат";
            case 1:
                result = "Полицай";
            case 2:
                result = "Доктор";
            case 3:
                result = "Офис работник";
            case 4:
                result = "Учител";
        }
        FinalPage object3 = new FinalPage();
        object3.setVisible(true);
    }                                        

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        int[] arr = new int[]{slider1, slider2, slider3, slider4, slider5};
        int max = 0;
        br = 0;
        for (i = 0; i < arr.length; i++) {
            if (arr[i] > max) {
                max = arr[i];
                br++;
            }
        }
        if (br == 0){
            result = "Адвокат";
        }
        else if (br == 1){
            result = "Полицай";
        }
        else if (br == 2){
            result = "Доктор";
        }
        else if (br == 3){
            result = "Офис работник";
        }
        else if (br == 4){
            result = "Учител";
        }
        /*switch (i) {
            case 0:
                result = "Адвокат";
            case 1:
                result = "Полицай";
            case 2:
                result = "Доктор";
            case 3:
                result = "Офис работник";
            case 4:
                result = "Учител";
        }*/
        jLabel7.setText(slider1 + "");
    }                                        

    public String getResult() {
        return result;
    }

